\pagestyle{empty}

\def\sectionsymbol{}
\def\subsectionsymbol{}
\def\subsubsectionsymbol{}

\def\startsection{%
  \def\@sectsymbol{\sectionsymbol}%
  \@startsection{section}{1}{\z@}%
  {0.1ex \@plus 1ex \@minus .2ex}%
  {0.001ex \@plus .0ex}%
  {\reset@font\normalsize\bfseries\mathversion{bold}}}

\def\section{%
  \def\@sectsymbol{\sectionsymbol}%
  \@startsection{section}{1}{\z@}%
  {3.5ex \@plus 1ex \@minus .2ex}%
  {0.1ex \@plus .0ex}%
  {\reset@font\normalsize}}
%  {\reset@font\large\bfseries\mathversion{bold}}}

\def\subsection{%
  \def\@sectsymbol{\subsectionsymbol}%
  \@startsection{subsection}{2}{\z@}%
  {2.5ex \@plus 1ex \@minus .2ex}%
  {1ex \@plus .2ex}%
  {\reset@font\normalsize}}

\def\l@section#1#2{%
  \ifnum \c@tocdepth >\z@%
    \addpenalty{\@secpenalty}%
    \addvspace{1.0em \@plus\p@}%
    \begingroup%
      \parindent\z@ \rightskip\@pnumwidth \parfillskip-\rightskip%
      \leavevmode\bfseries\mathversion{bold}%
      \setlength\@lnumwidth{1.5em}%
      \advance\leftskip\@lnumwidth \hskip-\leftskip%
      #1\nobreak\hfil\nobreak\hbox to\@pnumwidth{\hss#2}\par%
    \endgroup%
  \fi}

\def\@sect#1#2#3#4#5#6[#7]#8{%
  \ifnum #2>\c@secnumdepth%
    \let\@svsec\@empty%
  \else%
    \refstepcounter{#1}%
    \protected@edef\@svsec{\@sectsymbol\@seccntformat{#1}\relax}%
  \fi%
  \@tempskipa #5\relax%
  \ifdim \@tempskipa>\z@%
    \begingroup%
      #6{%
        \@hangfrom{\hskip #3\relax\@svsec}%
          \interlinepenalty \@M #8\@@par}%
    \endgroup%
    \csname #1mark\endcsname{#7}%
    \addcontentsline{toc}{#1}{%
      \ifnum #2>\c@secnumdepth%
      \else%
        \@sectsymbol\protect\numberline{\csname the#1\endcsname}%
      \fi%
      #7}%
  \else%
    \def\@svsechd{%
      #6{\hskip #3\relax%
      \@svsec #8}%
      \csname #1mark\endcsname{#7}%
      \addcontentsline{toc}{#1}{%
        \ifnum #2>\c@secnumdepth%
        \else%
          \@sectsymbol\protect\numberline{\csname the#1\endcsname}%
        \fi%
        #7}}%
  \fi%
  \@xsect{#5}}



